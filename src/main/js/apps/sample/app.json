{
  "properties": {
    "title": "${apptitle}"
  },
  "load": {
    "bundleLocations": [
      "localbundles",
      "bundles"
    ],
    "allowedBundles": [
      "system",
      "banner",
      "console",
      "custominfo",
      "notifier",
      "splashscreen",
      "templatelayout",
      "template-seasons",
      "theme-everlasting",
      "theme-winter",
      "theme-spring",
      "theme-summer",
      "theme-autumn",
      "map-init",
      "toolset",
      "mapflow",
      "dn_basemapslider",
      "console"
    ],
    "require": [
      "${app}/app"
    ],
    "i18n": [
      "bundle"
    ]
  },
  "bundles": {
      "banner": {
          "BannerWidget": {
              "label": "Developer Network",
              "imagePosition": "above",
              "image": "resource('${app}:/images/logo_conterra.png')",
              "imageWidth": 200,
              "link": "https://developernetwork.conterra.de"
          }
      },
    "custominfo": {
      "CustomInfoToggleTool": [
        {
          "id": "customInfoTool",
          "activateOnStartup": true,
          "title": "Information",
          "tooltip": "Information",
          "iconClass": "icon-custom-info-info",
          "toolClass": "ctInfo",
          "togglable": true,
          "widgetSettings": {
            "content": "${custominfo.content}",
            "window": {
              "title": "Information",
              "marginBox": {
                "w": 300,
                "h": 160,
                "l": 25,
                "b": 70
              },
              "draggable": true,
              "closable": true,
              "modal": false,
              "maximizable": false,
              "resizable": false
            }
          }
        }
      ]
    },
    "dn_basemapslider":{
      "BasemapSliderModel":{
        "showInMapflow": false
      }
    },
    "map-init": {
      "Config": {
        "basemaps": [
          {
            "id": "15",
            "title": "2015",
            "description": "This basemap showes the satellite picture of 2015",
            "basemap": {
              "type": "AGS_TILED",
